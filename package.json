{
  "name": "online-test",
  "version": "1.0.0",
  "main": "out.js",
  "scripts": {
    "start": "npm-run-all --parallel mock serve",
    "serve": "cd esbuild && node esbuild.dev.mjs",
    "build": "cd esbuild && node esbuild.prod.mjs",
    "api": "cd npm-scripts && npm run api https://github.com/JooffinHalli/online-test.git main",
    "mock": "cd serverMock && npm run mock",
    "gen": "cd serverMock && npm run gen",
    "del": "cd serverMock && npm run del",
    "clear": "cd serverMock && npm run clear"
  },
  "dependencies": {
    "antd": "^5.8.4",
    "mobx": "^6.10.0",
    "mobx-react-lite": "^4.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "esbuild": "^0.19.2",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.2.2"
  }
}
